.PROGRAM workshop4_2()
; Workshop 4 Relative Transformations 1

        SET loc[1] = TRANS(100,50,100,0,180,-45)
; Sätt ut loc[2], loc[3], loc[4] utifrån abolutvärdet av loc[1]

; workshop4_1 använd SHIFT

        SET loc[2] = SHIFT(loc[1] BY 100)
        SET loc[3] = SHIFT(loc[1] BY 100,50)
        SET loc[4] = SHIFT(loc[1] BY 0,50)



.END

